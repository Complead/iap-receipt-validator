{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "max-len": [1, 120, 2],
    "no-param-reassign": [2, { "props": false }],
    "no-continue": 0,
    "no-underscore-dangle": 0,
    "generator-star-spacing": 0,
    "class-methods-use-this": 0,
    "no-useless-computed-key": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
  },
  "env": {
    "jest": true,
    "es6": true,
  },
  "globals": {
    "fetch": false
  }
}
